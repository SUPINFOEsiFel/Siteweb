{% extends "::base.html.twig" %}
{% block stylesheets %}
    <meta name="description" content="La FEL (Fédération des Étudiants Lyonnais) est une association pour la cohésion et le développement de l'associatif des étudiants de Lyon. Elle trouve ses racines en 1989 dans la volonté de 7 associations étudiantes de Grandes Écoles et d’Universités de créer un organisme indépendant, apolitique, représentant tous les étudiants lyonnais.">
    <meta name="keywords" content="fédération;étudiants;lyonnais;lyon;association">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ asset('lib/bootstrap/css/bootstrap.min.css') }}" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('lib/bootstrap/css/bootstrap-theme.min.css') }}" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('lib/font-awesome/css/font-awesome.min.css') }}" type="text/css"/>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}">
    {% stylesheets '@FELFrontBundle/Resources/public/css/component.css' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {# appendStylesheets #}
    {% block appendStylesheets %}{% endblock %}
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('lib/jquery/jquery.min.js') }}" rel="script" type="application/javascript"></script>
    <script src="{{ asset('lib/bootstrap/js/bootstrap.min.js') }}" rel="script" type="application/javascript"></script>
    {% javascripts '@FELFrontBundle/Resources/public/js/modernizr.custom.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {# appendJavascripts #}
    {% block appendJavascripts %}{% endblock %}
{% endblock %}
